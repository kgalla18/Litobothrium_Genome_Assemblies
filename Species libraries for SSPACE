lib3 bowtie /tempdata3/KGalla/aenig/KJHR2D_L2_R1.fastq /tempdata3/KGalla/aenig/KJHR2D_L2_R2.fastq 516 0.75 FR
lib4 bowtie /tempdata3/KGalla/aenig/KJHR3D_D11_R1.mp.fastq.gz /tempdata3/KGalla/aenig/KJHR3D_D11_R2.mp.fastq.gz  2395 0.85 FR
lib5 bowtie /tempdata3/KGalla/aenig/KJHR3D_D6_R1.mp.fastq.gz /tempdata3/KGalla/aenig/KJHR3D_D6_R2.mp.fastq.gz  7539 0.85 FR
unpaired bowtie unpaired_KJHR1D_L1.fastq
unpaired bowtie unpaired_KJHR2D_L1.fastq
unpaired bowtie unpaired_KJHR2D_L2.fastq

